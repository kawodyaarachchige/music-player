<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="269.0" prefWidth="827.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.PlayerFormController">
    <children>
        <Button fx:id="btnAddMusic" layoutX="618.0" layoutY="94.0" mnemonicParsing="false" onAction="#btnAddMusicOnAction" prefHeight="49.0" prefWidth="128.0" text="ADD MUSIC" />
        <Button fx:id="btnPlay" layoutX="311.0" layoutY="179.0" mnemonicParsing="false" onAction="#btnPlayOnAction" text="PLAY" />
        <Button fx:id="btnStop" layoutX="95.0" layoutY="179.0" mnemonicParsing="false" onAction="#btnStopOnAction" text="STOP" />
        <Button fx:id="btnNext" layoutX="374.0" layoutY="179.0" mnemonicParsing="false" onAction="#btnNextOnAction" text="Next" />
        <Label fx:id="lblSongTitle" layoutX="92.0" layoutY="86.0" prefHeight="66.0" prefWidth="476.0">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <Button fx:id="btnPause" layoutX="157.0" layoutY="179.0" mnemonicParsing="false" onAction="#btnPauseOnAction" text="PAUSE" />
        <Button fx:id="btnResume" layoutX="229.0" layoutY="179.0" mnemonicParsing="false" onAction="#btnResumeOnAction" text="RESUME" />
        <Label fx:id="lblNotification" layoutX="98.0" layoutY="214.0" prefHeight="16.0" prefWidth="476.0" textFill="#e10a0a" />
        <Button fx:id="btnAddToFav" layoutX="618.0" layoutY="167.0" mnemonicParsing="false" onAction="#btnAddToFavOnAction" prefHeight="49.0" prefWidth="128.0" text="ADD to Fav" />
        <Label fx:id="lblCountdown" layoutX="92.0" layoutY="152.0" prefHeight="16.0" prefWidth="128.0" />
    </children>
</AnchorPane>